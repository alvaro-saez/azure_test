{
	"name": "2a - DYNAMIC BACKUP - individial",
	"properties": {
		"activities": [
			{
				"name": "dynamic_table_name",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.@{pipeline().parameters.tabla_name}"
					},
					{
						"name": "Destination",
						"value": "dbo.@{concat(pipeline().parameters.tabla_name, '_backup')}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": {
							"value": "IF OBJECT_ID('dbo.@{pipeline().parameters.tabla_name}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}', 'U') IS NOT NULL  \n   DROP TABLE dbo.@{pipeline().parameters.tabla_name}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}; ",
							"type": "Expression"
						},
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SQL_db_origin_backup",
						"type": "DatasetReference",
						"parameters": {
							"tabla_name": {
								"value": "@pipeline().parameters.tabla_name",
								"type": "Expression"
							},
							"database_name": {
								"value": "@pipeline().parameters.database_name_origin_backup",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQL_db_destination_backup",
						"type": "DatasetReference",
						"parameters": {
							"tabla_name": {
								"value": "@{pipeline().parameters.tabla_name}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}",
								"type": "Expression"
							},
							"database_name": {
								"value": "@pipeline().parameters.database_name_destination_backup",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"tabla_name": {
				"type": "String"
			},
			"database_name_origin_backup": {
				"type": "string"
			},
			"database_name_destination_backup": {
				"type": "string"
			}
		},
		"folder": {
			"name": "BACKUP | TRANSFER process/INDIVIDUAL/DYNAMIC/child pipelines"
		},
		"annotations": []
	}
}