{
	"name": "2a - CLONE DEV to DEV BACKUP - individial_copy1",
	"properties": {
		"activities": [
			{
				"name": "dynamic_table_name",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "dbo.@{pipeline().parameters.tabla_name}"
					},
					{
						"name": "Destination",
						"value": "dbo.@{concat(pipeline().parameters.tabla_name, '_backup')}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SQLdev_all_tables_parametrized",
						"type": "DatasetReference",
						"parameters": {
							"tabla_name": "@pipeline().parameters.tabla_name"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQLdev_all_tables_parametrized",
						"type": "DatasetReference",
						"parameters": {
							"tabla_name": {
								"value": "@{pipeline().parameters.tabla_name}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"tabla_name": {
				"type": "String"
			}
		},
		"folder": {
			"name": "BACKUP PROCESS/INDIVIDUAL/child pipelines"
		},
		"annotations": []
	}
}