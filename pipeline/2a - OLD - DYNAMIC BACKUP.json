{
	"name": "2a - OLD - DYNAMIC BACKUP",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.tableList",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "dynamic_table_name",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "dbo.@{item()}"
								},
								{
									"name": "Destination",
									"value": "dbo.@{item()}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "IF OBJECT_ID('dbo.@{item()}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}', 'U') IS NOT NULL  \n   DROP TABLE dbo.@{item()}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')};\n\n",
										"type": "Expression"
									},
									"writeBehavior": "insert",
									"sqlWriterUseTableLock": false,
									"tableOption": "autoCreate",
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "SQL_db_origin_backup",
									"type": "DatasetReference",
									"parameters": {
										"tabla_name_iterada": {
											"value": "@item()",
											"type": "Expression"
										},
										"database_name": {
											"value": "@pipeline().parameters.database_name_origin_backup",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "SQL_db_destination_backup",
									"type": "DatasetReference",
									"parameters": {
										"tabla_name_iterada": {
											"value": "@{item()}_@{formatDateTime(utcnow(),'yyyyMMddHHmm')}",
											"type": "Expression"
										},
										"database_name": {
											"value": "@pipeline().parameters.database_name_destination_backup",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"database_name_origin_backup": {
				"type": "string"
			},
			"database_name_destination_backup": {
				"type": "string"
			},
			"tableList": {
				"type": "array"
			}
		},
		"folder": {
			"name": "BACKUP | TRANSFER process/DYNAMIC - array tables Â¡BEST!/child pipelines/copias"
		},
		"annotations": []
	}
}